{
  "name": "2gis/php-k8s",
  "type": "project",
  "description": "Example of PHP web application for Kubernetes",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Igor Yatsevich",
      "email": "i.yatsevich@2gis.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.8",
    "ext-curl": "*",
    "ext-intl": "*",
    "ext-pdo": "*",
    "ext-pdo_pgsql": "*",
    "yiisoft/yii2-redis": "2.0.8",
    "yiisoft/yii2": "2.0.15",
    "yiisoft/yii2-composer": "~2.0.4",
    "graylog2/gelf-php": "^1.5",
    "mso/idna-convert": "1.1.0",
    "cbednarski/Pulse": "1.0.*",
    "codemix/yii2-streamlog": "^1.2",
    "psr/log": "^1.0",
    "kohkimakimoto/workerphp": "^0.6.0"
  },
  "require-dev": {
  },
  "provide": {
    "bower-asset/jquery": "3.2.*@stable",
    "bower-asset/jquery.inputmask": "~3.3.5",
    "bower-asset/punycode": "1.3.*",
    "bower-asset/yii2-pjax": "~2.0.1"
  },
  "scripts": {
    "post-create-project-cmd": [
      "yii\\composer\\Installer::setPermission"
    ]
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    },
    "writable": [
      "runtime",
      "runtime/logs"
    ],
    "executable": [
      "yii"
    ],
    "config": [
      "config/web.php"
    ]
  }
}
